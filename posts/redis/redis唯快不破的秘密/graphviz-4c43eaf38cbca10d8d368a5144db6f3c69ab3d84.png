				</div>
			</div>
		</div>
	</body>
	<script src="jquery-1.4.4.min.js">
	</script>
	<script>
		var link = document.createElement('link');
		link.rel = "stylesheet";
		link.type = "text/css";
		link.href = "./css/index_.css?t=" + new Date().getTime();
		document.getElementsByTagName("head")[0].appendChild(link);
		var script = document.createElement('script');
		script.src = "./js/index_.js?t=" + new Date().getTime();
		script.type = 'text/javascript';
		document.getElementsByTagName("body")[0].appendChild(script);
		function getQueryVariable(variable) {
			var query = window.location.search.substring(1);
			var vars = query.split("?");
			for (var i = 0; i < vars.length; i++) {
				var pair = vars[i].split("=");
				if (pair[0] == variable) {
					return pair[1]
				}
			}
			return (false)
		};
		getValueFromKeyByUrl(window.location.search, 'u') ? $('#aspan').html(decodeURIComponent(getValueFromKeyByUrl(window.location.search, 'u'))) : $('#aspan').html(window.atob(decodeURIComponent(getValueFromKeyByUrl(window.location.search, 'source'))));
		function getValueFromKeyByUrl(url, key) {
			if (url.indexOf(key) === -1) {
				return null
			}
			var arr_url = url.split('?');
			var base = arr_url[0];
			var arr_param = arr_url[1].split('&');
			for (var i = 0; i < arr_param.length; i++) {
				var paired = arr_param[i].split('=');
				if (paired[0] === key) {
					return paired[1]
				}
			}
			return null
		};
	</script>

</html>